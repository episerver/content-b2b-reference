@model IBlockViewModel<VideoBlock>

<div style="opacity:@Model.CurrentBlock.BlockOpacity;" class="@(Model.CurrentBlock.Padding + " vimeo-container " + Model.CurrentBlock.Margin + " " + @Model.CurrentBlock.BackgroundColor)">
    <video blockId="@(((IContent)Model.CurrentBlock).ContentLink.ID)" name="@(((IContent)Model.CurrentBlock).Name)" playsinline seeking controls>
        <source src="@Url.ContentUrl(Model.CurrentBlock.Video)" type="video/mp4">
    </video>
</div>